{
  "name": "starter-vite-react",
  "private": true,
  "version": "1.0.0",
  "description": "Opinionated Vite starter template with ESLint, Prettier, Stylelint and more.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabgrel10/vite-react-starter"
  },
  "keywords": [
    "vite",
    "react",
    "starter",
    "template"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "serve": "vite preview",
    "lint:eslint": "eslint . --ext .js,.jsx",
    "fix:eslint": "eslint --fix  . --ext .js,.jsx",
    "lint:stylelint": "stylelint **/*.{css,scss,jsx}",
    "fix:stylelint": "stylelint --fix  ./src/**/*.{css, scss,jsx}",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.2",
    "@mui/styles": "^5.11.2",
    "@reduxjs/toolkit": "^1.9.1",
    "react-redux": "^8.0.5",
    "axios": "^1.2.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.1.1",
    "sass": "^1.54.5",
    "slick-carousel": "^1.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0"
  }
}
